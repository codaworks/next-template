{
  "name": "prisma-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "db-push": "npx dotenv -e .env.local -- drizzle-kit push:pg",
    "db-pull": "npx dotenv -e .env.local -- drizzle-kit introspect:pg",
    "db-generate": "npx dotenv -e .env.local -- drizzle-kit generate:pg",
    "studio": "npx dotenv -e .env.local -- drizzle-kit studio",
    "postinstall": "npm run db-generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.30.4",
    "framer-motion": "^10.16.4",
    "iron-session": "^8.0.1",
    "next": "^14.0.0",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.69.4",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "autoprefixer": "^10",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "next-plugin-svgr": "^1.1.10",
    "postcss": "^8",
    "postcss-import": "^15.1.0",
    "prisma": "^5.7.0",
    "tailwindcss": "^3"
  }
}
