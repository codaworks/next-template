    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.69.4",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "autoprefixer": "^10",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "next-plugin-svgr": "^1.1.10",
    "postcss": "^8",
    "postcss-import": "^15.1.0",
