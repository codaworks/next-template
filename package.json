{
  "name": "prisma-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "email": "email dev --dir ./src/emails --port 3005",
    "migrate": "npx dotenv -e .env.local -e .env -- prisma migrate dev",
    "generate": "npx dotenv -e .env.local -e .env -- prisma generate",
    "db-sync": "npx dotenv -e .env.local -e .env -- prisma db push",
    "db-studio": "npx dotenv -e .env.local -e .env -- prisma studio",
    "pregenerate": "npx prisma-import -s src/db/**/*.prisma -o prisma/schema.prisma -f",
    "postinstall": "npm run generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@react-email/components": "^0.0.15",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.22",
    "iron-session": "^8.0.1",
    "next": "^14",
    "react": "^18",
    "react-dom": "^18",
    "react-email": "^2.1.1-canary.1",
    "resend": "^3.2.0",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "autoprefixer": "^10",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "next-plugin-svgr": "^1.1.10",
    "postcss": "^8",
    "postcss-import": "^15.1.0",
    "prisma": "^5.11.0",
    "prisma-import": "^1.0.5",
    "tailwindcss": "^3.4.2",
    "typescript": "^5.4.5"
  }
}
